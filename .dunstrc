[global]
    font = Terminus 9
    allow_markup = yes
    format = "<b>%s</b>\n%b"
    sort = yes
    indicate_hidden = yes
    alignment = left
    bounce_freq = 0
    show_age_threshold = 60
    word_wrap = yes
    ignore_newline = no
    geometry = "300x15-30+40"
    transparency = 0
    idle_threshold = 120
    monitor = 0
    follow = mouse
    sticky_history = yes
    line_height = 0
    separator_height = 2
    padding = 9
    horizontal_padding = 2
    separator_color = frame
    startup_notification = false
    dmenu = /usr/bin/dmenu -p dunst
    browser = /usr/bin/firefox -new-tab
    icon_position = left

[frame]
    width = 6
    color = "#F1EDE1"

[shortcuts]
    close = ctrl+space
    close_all = ctrl+shift+space
    history = ctrl+grave
    context = ctrl+shift+period

[urgency_low]
    background = "#F1EDE1"
    foreground = "#B084A7"
    timeout = 10

[urgency_normal]
    background = "#F1EDE1"
    foreground = "#B084A7"
    timeout = 10

[urgency_critical]
    background = "#F1EDE1"
    foreground = "#B084A7"
    
