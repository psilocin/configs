[active]
  id text
  source window_title
  forground #B084A7
  background #EED4E8

[desktops]
  id radio
  source workspaces
  forground #B084A7
  background #F1EDE1

[time]
  id text
  source clock
  format %T
  forground #B084A7
  background #E5C2B7

[weather]
  id weather
